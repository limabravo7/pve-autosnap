# ----------------------------------------------------------------------------
# bash completion for pve-autosnap
# ----------------------------------------------------------------------------

_pve_autosnap_completion()
{
    local suggestions=""

    COMPREPLY=("${suggestions[@]}")
}

# ----------------------------------------------------------------------------
# complete pve-autosnap
# ----------------------------------------------------------------------------

complete -o nospace -F _pve_autosnap_completion pve-autosnap

# ----------------------------------------------------------------------------
# End-Of-File
# ----------------------------------------------------------------------------
